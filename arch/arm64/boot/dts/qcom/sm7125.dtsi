#include "sc7180.dtsi"

/* SM7125 uses Kryo 465 instead of Kryo 468 */
&CPU0 {
	compatible = "qcom,kryo465";
	/delete-property/ operating-points-v2;

	d-cache-size = <0x8000>;
	i-cache-size = <0x8000>;
};

&L2_0 {
	cache-size = <0x10000>;
	cache-level = <2>;
};

&L3_0 {
	cache-size = <0x100000>;
	cache-level = <3>;
};

